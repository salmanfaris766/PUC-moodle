1)
#include <stdio.h>

int main() {
    int a, b;

    scanf("%d %d", &a, &b);

    int c, d;
    c = a % 10;
    d = b % 10;

    if (c == d) {
        printf("true");
    } else {
        printf("false");
    }

    return 0;
}
2)
#include <stdio.h>

int main() {
    int n;

    scanf("%d", &n);

    if (n % 2 != 0) {
        printf("Weird");
    } else if (n >= 2 && n <= 5) {
        printf("Not Weird");
    } else if (n >= 6 && n <= 20) {
        printf("Weird");
    } else {
        printf("Not Weird");
    }

    return 0;
}
3)
#include <stdio.h>

int main() {
    int a, b, c;

    scanf("%d %d %d", &a, &b, &c);

    if ((a * a) == (b * b) + (c * c)) {
        printf("yes");
    } else if ((b * b) == (a * a) + (c * c)) {
        printf("yes");
    } else if ((c * c) == (a * a) + (b * b)) {
        printf("yes");
    } else {
        printf("no");
    }

    return 0;
}
